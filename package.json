{
    "name": "tjdraper.com",
    "version": "6.0.0",
    "private": true,
    "scripts": {
        "dev": "yarn run publish && next dev",
        "build": "yarn run publish && next build",
        "start": "yarn run publish && next start",
        "lint": "next lint && stylelint --allow-empty-input 'pages/**/*.css' 'styles/**/*.css'",
        "build-cli": "rm -rf cliSrc/dist && yarn tsc --project tsconfig-cli.json",
        "publish": "yarn build-cli && yarn publish-blog-images && yarn publish-blog-pages",
        "publish-blog-images": "./cli publish blog-images",
        "publish-blog-pages": "./cli publish blog-pages"
    },
    "oclif": {
        "bin": "cli",
        "dirname": "cliSrc",
        "commands": "./cliSrc/dist/cliSrc/src",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-plugins"
        ],
        "topicSeparator": " ",
        "topics": {
            "publish": {
                "description": "Commands for publishing items"
            }
        }
    },
    "files": [],
    "dependencies": {
        "@headlessui/react": "^1.7.4",
        "@heroicons/react": "^2.0.13",
        "@mdx-js/loader": "^2.0.0-rc.2",
        "@mdx-js/react": "^2.0.0-rc.2",
        "@next/mdx": "^13.0.2",
        "@oclif/core": "^1.20.4",
        "@oclif/plugin-help": "^5.1.18",
        "@oclif/plugin-plugins": "^2.1.7",
        "cli-color": "^2.0.3",
        "cli-output": "^1.0.0",
        "fs-extra": "^10.1.0",
        "gray-matter": "^4.0.3",
        "marked": "^4.2.2",
        "next": "13.0.2",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "rehype-raw": "^6.1.1",
        "rehype-sanitize": "^5.0.1",
        "rehype-stringify": "^9.0.3",
        "remark-frontmatter": "^4.0.1",
        "remark-gfm": "^3.0.1",
        "remark-mdx-frontmatter": "^2.1.1",
        "remark-parse": "^10.0.1",
        "remark-parse-frontmatter": "^1.0.3",
        "remark-rehype": "^10.1.0",
        "remark-smartypants": "^2.0.0",
        "typescript": "4.8.4",
        "unified": "^10.1.2"
    },
    "devDependencies": {
        "@babel/core": "^7.20.2",
        "@babel/eslint-parser": "^7.19.1",
        "@babel/preset-env": "^7.20.2",
        "@tailwindcss/typography": "^0.5.7",
        "@tailwindcss/ui": "^0.7.2",
        "@types/cli-color": "^2.0.2",
        "@types/fs-extra": "^9.0.13",
        "@types/marked": "^4.0.7",
        "@types/mdast": "^3.0.10",
        "@types/mdx": "^2.0.3",
        "@types/node": "^18.11.9",
        "@types/react": "^18.0.25",
        "@types/react-dom": "^18.0.8",
        "@types/revalidator": "^0.3.8",
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@typescript-eslint/parser": "^5.42.0",
        "autoprefixer": "^10.4.13",
        "eslint": "^8.26.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-next": "^13.0.2",
        "eslint-plugin-disable": "^2.0.3",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-no-jquery": "^2.7.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.31.10",
        "eslint-plugin-react-hooks": "^4.6.0",
        "postcss": "^8.4.18",
        "stylelint": "^14.14.1",
        "stylelint-config-standard": "^29.0.0",
        "tailwindcss": "^3.2.2"
    }
}
